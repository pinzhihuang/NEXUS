# OpenRouter API Key (Required) 
# Get your API key from https://openrouter.ai/keys 
OPENROUTER_API_KEY=sk-or-v1-YOUR_OPENROUTER_API_KEY_HERE 
 
# Google Custom Search API (Optional - for enhanced article discovery) 
# GOOGLE_API_KEY=YOUR_GOOGLE_API_KEY_FOR_PSE 
# CUSTOM_SEARCH_ENGINE_ID=YOUR_GOOGLE_PSE_CX_ID 
 
# Google Docs API (Optional - for exporting to Google Docs) 
# TARGET_GOOGLE_DOC_ID=YOUR_GOOGLE_DOCUMENT_ID_HERE 
 
# Model Configuration (Optional - defaults are set in config.py) 
# GEMINI_FLASH_MODEL=google/gemini-2.5-flash 
# GEMINI_PRO_MODEL=google/gemini-2.5-pro 
 
# News Collection Settings (Optional) 
# NEWS_START_DATE=2025-08-01 
# MAX_CATEGORY_PAGES_TO_SCAN=20 
# MAX_FINAL_REPORTS=10 
 
# Flask Configuration 
FLASK_ENV=production 
SECRET_KEY=change-this-to-a-random-secret-key-in-production 
 
# Railway automatically provides PORT, no need to set it
